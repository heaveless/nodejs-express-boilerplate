{
  "name": "project",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist && npm run copy",
    "clean": "rimraf ./dist",
    "copy": "copyfiles -u 1 ./src/**/*.html ./dist",
    "start": "node -r ./scripts/prebuild.js ./dist/server.js",
    "start:dev": "nodemon --config nodemon.json",
    "orm:seed": "ts-node ./src/commands/orm.command.ts -s",
    "orm:seed:dev": "ts-node -r ./scripts/prebuild.js ./src/commands/orm.command.ts -s",
    "db:seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/faker": "5.5.9",
    "@types/nodemailer": "^6.4.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "copyfiles": "^2.4.1",
    "dotenv": "^14.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "apollo-server-express": "^3.6.2",
    "body-parser": "^1.19.1",
    "class-validator": "^0.13.2",
    "commander": "^8.3.0",
    "express": "^4.17.2",
    "faker": "5.5.3",
    "graphql": "^15.3.0",
    "inversify": "^6.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "inversify-express-utils": "^6.4.3",
    "inversify-inject-decorators": "^3.1.0",
    "inversify-logger-middleware": "^3.1.0",
    "lodash": "^4.17.21",
    "nodemailer": "^6.7.2",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "swagger-express-ts": "^1.1.0",
    "swagger-ui-dist": "^4.2.1",
    "tsconfig-paths": "^3.12.0",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.41",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^8.3.2"
  }
}
